<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="nav-bar">
      <% if user_signed_in? %>
       <h4><%= current_user.email %> | <%= link_to "Log out", destroy_user_session_path, method: :delete %></h4>
      <%else%>
      <h3 align="right"><%=  link_to 'Sign_in',  new_user_session_path%></h3>
      <h3 align="right"><%=  link_to 'Sign_Up',  new_user_registration_path%></h3>
      <% end %>
    </div>
    <% if  notice %>
    <p class="notice"><%= notice %></p>
    <% end %>
    <% if  alert %>
    <p class="alert"><%= alert %></p>
    <% end %>
    <%= yield %>
 
  </body>
</html>
